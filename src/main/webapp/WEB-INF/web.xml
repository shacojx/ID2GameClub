<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterSevlet</servlet-name>
        <servlet-class>Controller.RegisterSevlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccSevlet</servlet-name>
        <servlet-class>Controller.AccSevlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Authentication</servlet-name>
        <servlet-class>Controller.Authentication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckIndex</servlet-name>
        <servlet-class>Controller.CheckIndex</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterSevlet</servlet-name>
        <url-pattern>/RegisterSevlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccSevlet</servlet-name>
        <url-pattern>/AccSevlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Authentication</servlet-name>
        <url-pattern>/Authentication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckIndex</servlet-name>
        <url-pattern>/CheckIndex</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/Login/Login.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
